class Solution {
public:
    string removeOccurrences(string s, string part) {
        
        int index = s.find(part);
        int len = part.size();

        while( index >= 0 && index < s.size() )
        {
            s.erase( index, len );
            index = s.find(part);
        }
        
       return s;
    }
};
// LeetCode 1910
